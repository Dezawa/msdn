<div class="img_title">過去24時間の地震</div>
<div class="img_tile">
<a href="/quake/map/" style="text-decoration: none;">
<div class="quakes24"></div>
</a>
</div>
</div>
<style>
.quake {
fill-opacity: 0.5;
stroke-width: 1;
stroke-opacity: 0.7;
}
</style>
<script>
var quakeMap = function(){
    var w = 240;
    var h = 180;
    var now = new Date();

    // SVG要素(土台)
    var svg = d3.select(".quakes24")
          .append("svg")
          .attr("width", w)
          .attr("height", h)
          .style("position", "relative")
          .style("opacity", 0)
          .style("overflow", "hidden")
          .style("background-color", "#ffffff");

    // メルカトル図法に設定
    var projection = d3.geo.mercator()
        .center([135, 35.5])
        .translate([w/2, h/2])
        .scale(370);

    // 日本地図データ読み込み
    d3.json("/json/japan.json", function(json) {
        var japan = topojson.object(json, json.objects.japan).geometries;
        // 日本地図プロット
        plotJapan(japan);

        d3.json("/json/quakes24h.json", function(json) {
            // 地震プロット
            var quakes = json;
            plotQuake(quakes);

            plotTitle(quakes);

            // フェードイン
            svg.transition()
            .duration(500)
            .style("opacity", 1);
        });
    });

    // 日本地図
    var plotJapan = function(japan) {
        var path = d3.geo.path()
            .projection(projection);

        svg.selectAll("path")
        .data(japan)
        .enter()
        .append("path")
        .attr("d", path)
        .style("fill", function(d){
            if (d.properties.geonunit == 'Japan') {
                return "#2ca02c";
            } else {
                return "#867d3f";
            }
        });
    };

    // 地震
    var plotQuake = function(quakes) {
        svg.selectAll(".quake")
        .data(quakes)
        .enter()
        .append("circle")
        .attr("class", function(d, i){
            if (i == quakes.length - 1) {
                return "quake last";
            } else {
                return "quake";
            }
        })
        .attr("transform", function(d) {
           return "translate(" + projection([d.lng, d.lat]) + ")";
        })
        .attr("r", function(d) {
            if (d.hidden) {
                return 0;
            } else {
                return magnitudeRadius(d.magnitude);
            }
        })
        .style("fill", function(d) {
            return depthColor(d.depth);
        })
        .style("stroke", function(d) {
            return depthColor(d.depth);
        })
    };

    var plotTitle = function(quakes) {
        var countQuake = quakes.length;

        svg.append("rect")
        .attr("x", 0)
        .attr("y", 0)
        .attr("width", 120)
        .attr("height", 30)
        .style("fill", "#000000")
        .style("fill-opacity", 0.5);

        svg.append("text")
        .text("地震観測回数: " + countQuake + "回")
        .attr("class", "countQuake")
        .attr("transform", "translate(7, 20)")
        .attr("font-size", "12px")
        .style("fill", "#ffffff");
    };

    var magnitudeRadius = function(magnitude) {
        var alpha = 1.2;
        return magnitude * alpha;
    };

    var depthColor = function(depth) {
        if (depth >= 250) {
            depth = 250;
        }
        return "hsl(" + depth + ",100%,50%)";
    };
};
quakeMap();
</script>
